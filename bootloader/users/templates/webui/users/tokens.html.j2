{% extends 'webui/users/layout.html.j2' %}

{% block content %}
          <div class="col-sm-9 col-sm-offset-2 col-md-10 col-md-offset-2 main">
            <table class="table">
              <tr>
                <th>Key</th>
                <th>Created</th>
              </tr>
              {% for token in tokens %}
              <tr>
                <td>{{ token.key }}</td>
                <td>{{ token.created|timesince }} ago</td>
              </tr>
              {% endfor %}
            </table>
            <form method="post">
              {% csrf_token %}
              <button type="submit" class="btn btn-success btn-default">(Re)Generate</button>
            </form>
          </div>
{% endblock %}
